algoritmo "EMPREGO"

var
   SALARIO: REAL
      inicio

   ESCREVA ( "Digite o salário: " )   //String

   LEIA ( SALARIO )
   
   
   SE ( SALARIO <= 2000 ) ENTAO
        ESCREVA ("Candidato aprovado para primeira fase da condicional") //teste lógico com resultado verdadeiro.
       //condicional simples


        FIMSE
        
        fimalgoritmo